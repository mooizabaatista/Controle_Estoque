@model FrenteServico


<div class="row bg-secondary text-center py-3 mt-4 mb-3 text-light">
    <div class="col-12">
        <h2>Cadastro de Frente de Serviços</h2>
    </div>
</div>


<form asp-action="Create" asp-controller="FrenteServicos" method="post">
    <div class="form-group">
        <label>Nome</label>
        <input asp-for="Nome" class="form-control" required />
    </div>

    <div class="form-group my-3">
        <input type="submit" value="Cadastrar" class="btn btn-success" />
        <a asp-action="Index" asp-controller="FrenteServicos" class="btn btn-primary">Voltar</a>
    </div>
</form>